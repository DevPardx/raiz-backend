{
  "name": "backend",
  "version": "1.0.0",
  "description": "Ra√≠z - Real State Backend",
  "keywords": [
    "Real State",
    "TypeScript",
    "Express",
    "PostgreSQL"
  ],
  "homepage": "https://github.com/DevPardx/raiz-backend#readme",
  "bugs": {
    "url": "https://github.com/DevPardx/raiz-backend/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevPardx/raiz-backend.git"
  },
  "license": "ISC",
  "author": "Diego Pardo",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/config/typeorm.config.ts",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/config/typeorm.config.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/config/typeorm.config.ts",
    "migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show -d src/config/typeorm.config.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.3",
    "@types/web-push": "^3.6.4",
    "eslint": "^9.39.1",
    "jest": "^29.7.0",
    "jiti": "^2.6.1",
    "nodemon": "^3.1.11",
    "prettier": "3.7.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "i18next": "^25.7.1",
    "jsonwebtoken": "^9.0.3",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "rate-limit-redis": "^4.3.1",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.27",
    "web-push": "^3.6.7",
    "winston": "^3.18.3"
  }
}
